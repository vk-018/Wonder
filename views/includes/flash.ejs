<% if(succMessage && succMessage.length){ %>
<div class="alert alert-success alert-dismissible fade show col-10 offset-1 mt-3" role="alert">
  <%= succMessage%>
  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>

</div>
<% } %>

<% if(failMessage && failMessage.length){ %>
<div class="alert alert-danger alert-dismissible fade show col-10 offset-1 mt-3" role="alert">
  <%= failMessage%>
  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>

</div>
<% } %>

 
